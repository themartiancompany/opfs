      "^2.16.0",
    "dlx":
      "^0.2.1",
    "eslint":
      "^9.32.0",
    "parcel":
      "^2.16.0",
    "rimraf":
      "^6.1.0",
    "rollup":
      "^4.52.5",
    "rollup-plugin-dts":
